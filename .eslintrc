---
    extends: "eslint:recommended"
    env:
        browser: true
    parserOptions:
        ecmaVersion: 5
        sourceType: "script"
    globals:
        angular: false
        ionic: false
    rules:
        semi: 2
        no-alert: 1
        indent:
          - warn
          - 2
        no-tabs: 2
        no-empty: 1
        no-unreachable: 2
        no-eval: 2
        no-extend-native: 2
        no-extra-bind: 1
        no-fallthrough: 2
        no-invalid-this: 2
        no-new: 2
        no-console: 0
